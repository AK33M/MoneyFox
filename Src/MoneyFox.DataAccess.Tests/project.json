{
    "version": "1.0.0-*",

    "testRunner": "xunit",

    "dependencies": {
        "dotnet-test-xunit": "2.2.0-*",
        "Microsoft.EntityFrameworkCore": "1.1.1",
        "Microsoft.EntityFrameworkCore.Sqlite": "1.1.1",
        "xunit": "2.2.0",
        "NETStandard.Library": "1.6.1"
    },

    "frameworks": {
        "netcoreapp1.1": {
            "imports": [
                "dnxcore50",
                "portable-net45+win8"
            ],

            "dependencies": {
                "Microsoft.NETCore.App": {
                    "version": "1.1.0",
                    "type": "platform"
                },
                "MoneyFox.DataAccess": {
                    "target": "project"
                },
                "MoneyFox.Foundation": {
                    "target": "project"
                }
            }
        }
    },
    "buildOptions": {
        "copyToOutput": {
            "include": [ "xunit.runner.json" ]
        }
    }
}